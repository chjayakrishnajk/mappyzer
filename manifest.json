{
  "manifest_version": 3,
  "name": "JEE Main PYQ",
  "version": "1.0",
  "description": "Tracks Marks app PYQs",
  "permissions": ["activeTab","storage"],
  "host_permissions": ["https://web.getmarks.app/"],
  "action" : {
    "default_popup": "popup.html"
  }
  ,
"content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
         "run_at": "document_start"
    }
  ]
}

